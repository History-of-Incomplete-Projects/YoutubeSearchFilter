{
    "name": "Youtube Search Filter",
    "description": "Filter out blocked channel from search results",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://*/*", 
        "https://*/*"
      ]
  }